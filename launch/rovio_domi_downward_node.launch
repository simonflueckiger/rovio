<?xml version="1.0" encoding="UTF-8"?> 
<launch>
  <group ns="blackbird" >
    <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
    <param name="filter_config" value="$(find rovio)/cfg/rovio_custom_domi_downward.info"/>
    <param name="camera0_config" value="$(find rovio)/cfg/domi_downward_cam0.yaml"/>
    <param name="camera1_config" value="$(find rovio)/cfg/domi_downward_cam1.yaml"/>
    
    <remap from="imu0" to="/uvc_camera/cam_0/imu"/>
    <remap from="cam0/image_raw" to="/uvc_camera/cam_0/image_raw"/>
    <remap from="cam1/image_raw" to="/uvc_camera/cam_0/image_raw"/>
    </node>
  </group>
</launch>
